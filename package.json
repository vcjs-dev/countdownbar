{
  "name": "countdownbar",
  "version": "1.5.3",
  "description": "A countdown bar library for web app.",
  "author": {
    "name": "elenh"
  },
  "type": "module",
  "main": "./lib/countdownbar.cjs",
  "module": "./lib/countdownbar.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/countdownbar.js",
      "require": "./lib/countdownbar.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "countdown",
    "timer",
    "component",
    "vue",
    "react",
    "counter",
    "web"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/vcjs-dev/countdownbar"
  },
  "homepage": "https://github.com/vcjs-dev/countdownbar",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "release:no-build": "changelogen --release && npm publish --access=public && git push --follow-tags",
    "release": "npm run build:lib && npm run build:type && npm run release:no-build",
    "build:app": "cross-env BUILD_TYPE=app vite build",
    "build:lib": "cross-env BUILD_TYPE=lib vite build",
    "build:type": "rollup -c rollup.dts.config.js"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "changelogen": "^0.5.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "demoify": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prismjs": "^1.29.0",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "sass": "^1.69.6",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-svg4vue": "^3.1.0",
    "vitest": "^1.1.1",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "fourdom": "^1.3.0"
  }
}